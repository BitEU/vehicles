function onTick()
	
	dist=gn(1)
	x=gn(2)
	y=gn(3)
	fov=gn(4)
	size=gn(5)
	hp=gn(6)
	rdrfov=gn(7)*360
	rdrmax=gn(8)
	lock=gb(1)
	blink=gb(2)
	power=gb(3)
	radar=gb(4)
	IR=gb(5)
	IFF=gb(6)
	
	
	dp=(dist/rdrmax)*32
	
	dfov=((135-1.43)*(1-fov))+1.43
	dx=(x*360)
	dy=(y*360)
	
	sx=(dx/dfov)*32
	sy=(dy/dfov)*32
	
	ret=((rdrfov/dfov)*32)+1
		
end

function onDraw()
if power then
	if not IR then
	
	if hp ~=0 then
	SC(255,255,255,100)
	DL(30,0,31.25,0.25)
	DL(30,31,31.25,31.25)
	DL(31,1,31.25,30.25)
	if lock then
	SC(255,0,0)
	DL(29,30-dp,31.25,30.25-dp)
	DR((15+sx)-size/2,(14-sy)-size/2,size+2,size+2)
	if blink then
	SC(0,255,0)
	else
	SC(0,255,0,100)
	end
	DR(15.5-(ret/2),15.5-(ret/2),ret,ret)
	else 
	SC(0,255,0,200)
	DR(15.5-(ret/2),15.5-(ret/2),ret,ret)
	end
	else
	end
	
	else
	
	if hp ~=0 then
	SC(255,255,255,200)
	DL(0,0,1.25,0.25)
	DL(0,31,1.25,31.25)
	DL(0,1,0.25,30.25)
	if lock then
	SC(255,0,0)
	DL(0,30-dp,2,30.25-dp)
	DR((15+sx)-size/2,(14-sy)-size/2,size+2,size+2)
	if blink then
	SC(0,0,0,200)
	else
	SC(0,0,0,100)
	end
	DR(15.5-(ret/2),15.5-(ret/2),ret,ret)
	else 
	SC(0,0,0,200)
	DR(15.5-(ret/2),15.5-(ret/2),ret,ret)
	end
	else
	SC(0,0,0)
	DRF(0,0,32,32)
	SC(255,0,0)
	DTX(5,13,"EMPTY")
	end
	end

	if IFF then
	SC(0,0,255)
	DTX(9,13,"IFF")
	else end


else
end
	
