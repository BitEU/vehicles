timer=0

function onTick()
	InputMax = property.getNumber("Input Max")
	Input1 = input.getNumber(1)/InputMax
	Input2 = input.getNumber(2)/InputMax
	Splash = property.getBool("Splash Enable")
	Horizontal = property.getBool("Horizontal Mode")
	Taper = property.getBool("Taper Mode")
	TaperLabelled = property.getBool("Taper Mode Labelled?")
	TaperLabel = property.getText("Taper Mode Label")
	TaperValueOn = property.getBool("Show Taper Value")
	HorizontalValue = property.getBool("Horizontal Value")
	if InputMax &lt; 10 then
	TaperValue = string.format("%0.0f", input.getNumber(1)*100) .." %"
	HorizontalValue1 = string.format("%0.0f", input.getNumber(1)*100) .." %"
	HorizontalValue2 = string.format("%0.0f", input.getNumber(2)*100) .." %"
	else
	TaperValue = string.format("%0.1f", input.getNumber(1))
	HorizontalValue1 = string.format("%0.1f", input.getNumber(1))
	HorizontalValue2 = string.format("%0.1f", input.getNumber(2))
	end
	Green1 = {0,131,49}
	Green2 = {0,158,29}
	Green3 = {0,226,50}
	Yellow1 = {255,231,39}	
	Orange1 = {255,165,2}	
	Orange2 = {255,127,39}
	Red1 = {255,0,49}
	
	Inputs = {Input1, Input2}
	
	
	--This is the table for segment colours. If you want to change a segment colour, edit the corresponding table entry below.
	scC = {Green1, Green1, Green2, Green2, Green3, Green3, Yellow1, Yellow1, Orange1, Orange1, Orange2, Orange2, Red1 ,Red1}

	segmenttb = {}
	
	if MonitorOn and timer&lt;255 then
    	timer=timer+1
  	end
end	
	function gFix(r,g,b)
		r=r^2.2/255^2.2*r
		g=g^2.2/255^2.2*g
		b=b^2.2/255^2.2*b
		return r,g,b
	end

	function Gauge(inputnum)	
		inputX = Inputs[inputnum]
		
		
		if Horizontal == false then
		offset = ((inputnum-1)*15)
		
		Yoffset = 0 else
		
		
		Yoffset = ((inputnum-1)*15)
		
		offset = 0
		
		end
		
		if inputnum &gt; 4 then
		Yoffset = 32
		end
		
		for i = 14,1,-1 do
			screen.setColor(10,10,10)
			if Horizontal == false then
			screen.drawRectF(2+offset,(30-i*2)+Yoffset,13,1)
			elseif Taper == false then
			screen.drawRectF((1+i*2)+offset,2+Yoffset,1,13)
			else
			screen.drawRectF((1+i*2)+offset,2+Yoffset,1,5+i)
			end
			
			if Horizontal == false and inputX &gt; 0.071428*i then
			screen.setColor(gFix(table.unpack(scC[i])))
			screen.drawRectF(2+offset,(30-i*2)+Yoffset,13,1)
			elseif Taper == false and inputX &gt; 0.071428*i then
			screen.setColor(gFix(table.unpack(scC[i])))
			screen.drawRectF((1+i*2)+offset,2+Yoffset,1,13)
			elseif inputX &gt; 0.071428*i then
			screen.setColor(gFix(table.unpack(scC[i])))
			screen.drawRectF((1+i*2)+offset,2+Yoffset,1,5+i) 
			end                                                           
		end
	end
	
	
function onDraw()

	MonitorOn=true
	w = screen.getWidth()
	h = screen.getHeight()						
	
	
	Gauge(1)
	screen.setColor(255,255,255)
	
	if Taper and TaperLabelled then
	screen.drawTextBox(2, 26, w-4, 5, TaperLabel)
	end
	if Taper and TaperValueOn then
	screen.drawTextBox(2, 20, 100, 5, TaperValue)
	end
	

	
	if Taper == false then 
	Gauge(2)
	end
	
	if Horizontal == true and HorizontalValue == true and Taper == false then
	screen.setColor(0,0,0)
	screen.drawRectF(3,2,w,7)
	screen.drawRectF(3,17,w,7)
	screen.setColor(255,255,255)
	screen.drawTextBox(3,3,w,5,HorizontalValue1)
	screen.drawTextBox(3,18,w,5,HorizontalValue2)
	end
	
	
	if Splash and timer &lt; 150 then 
	screen.setColor(0,0,0)
	screen.drawRectF(0,0,w,h)	
    screen.setColor(255,255,255)
	screen.drawTextBox(1,8,w-2,h-8,"Script  by   Xebec")
	end
	
	if Splash and timer &gt; 149 and timer &lt; 250 then
	
	screen.setColor(0,0,0)
	screen.drawRectF(0,0,w,h)
	screen.setColor(gFix(49,123,196))
	screen.drawLine(2, 27, 30, 0)
	screen.drawLine(3, 27, 31, 0)
	screen.drawLine(4, 27, 32, 0)
	screen.drawLine(5, 27, 33, 0)
	screen.drawLine(6, 27, 34, 0)
	screen.drawLine(7, 4, 22, 31)
	screen.drawLine(8, 4, 23, 31)
	screen.drawLine(9, 4, 24, 31)
	screen.drawLine(10, 4, 25, 31)
	screen.drawLine(11, 4, 26, 31)
	screen.setColor(0,0,0)
	screen.drawRectF(0,8.75,16,3)
	screen.drawRectF(0,27.25,32,4.75)
	screen.drawRectF(0,0,32,6.25)
	end
end'><pos x="-11.25" y="11.75"/><in1 component_id="77" node_index="0"/><in2 component_id="0" node_index="0"/><out1/><out2/></object></c><c type="40"><object id="77" count="8" offset="0"><pos x="-14" y="10"/><inc component_id="0" node_index="0"/><in1 component_id="3" node_index="0"/><in2 component_id="4" node_index="0"/><in3 component_id="0" node_index="0"/><in4 component_id="0" node_index="0"/><in5 component_id="0" node_index="0"/><in6 component_id="0" node_index="0"/><in7 component_id="0" node_index="0"/><in8 component_id="0" node_index="0"/><out1/></object></c><c type="56"><object id="80" script='--- This whole script is for the Right bar gauge
function onTick()
	Input2 = input.getNumber(2)
	
	Green1 = {0,131,49}
	Green2 = {0,158,29}
	Green3 = {0,226,50}
	Yellow1 = {255,231,39}
	Orange1 = {255,165,2}
	Orange2 = {255,127,39}
	Red1 = {255,0,49}
	
	
	scC = {Green1, Green1, Green2, Green2, Green3, Green3, Yellow1, Yellow1, Orange1, Orange1, Orange2, Orange2, Red1 ,Red1}

	
	segmenttb = {"","","","","","","","","","","","","",""}
	
end	
	function gFix(r,g,b)
		r=r^2.2/255^2.2*r
		g=g^2.2/255^2.2*g
		b=b^2.2/255^2.2*b
		return r,g,b
	end

	function mainfunction(input)
		for i = 1,14,1 do
			if input &gt; 0.071428*i then
				segmenttb[i] = true
			end
		end
	end

	function mk2(input)	
		for i = 14,1,-1 do
			screen.setColor(10,10,10)
			screen.drawRectF(17,30-i*2,13,1)
			if input &gt; 0.071428*i then
			screen.setColor(gFix(table.unpack(scC[i])))
			screen.drawRectF(17,30-i*2,13,1) 
			end                                                           
		end
	end
	

function onDraw()
	
	w = screen.getWidth()
	h = screen.getHeight()						
	mk2(Input2)
			
end	'><pos x="-2.25" y="19.25"/><in1 component_id="0" node_index="0"/><in2 component_id="0" node_index="0"/><out1/><out2/></object></c><c type="33"><object id="83" n="Splash Enable" on="Enabled" off="Disabled" v="true"><pos x="-13.75" y="21"/><out1/></object></c><c type="33"><object id="84" n="Horizontal Mode" on="Enable" off="Disable" v="true"><pos x="-13.75" y="20.25"/><out1/></object></c><c type="33"><object id="85" n="Taper Mode" on="Enable (Horizontal must be enabled)" off="Disable" v="false"><pos x="-13.75" y="19.5"/><out1/></object></c><c type="58"><object id="86" n="Taper Mode Label" v="THRTL"><pos x="-13.75" y="18"/></object></c><c type="33"><object id="87" n="Taper Mode Labelled?" on="Enable" off="Disable" v="true"><pos x="-13.75" y="18.75"/><out1/></object></c><c type="33"><object id="88" n="Show Taper Value" on="Enable" off="Disable" v="true"><pos x="-13.75" y="17.25"/><out1/></object></c><c type="34"><object id="89" n="Input Max"><pos x="-13.75" y="16.5"/><out1/><v text="1" value="1"/></object></c><c type="33"><object id="90" n="Horizontal Value" on="Enable" off="Disable" v="true"><pos x="-13.75" y="15.75"/><out1/></object></c></components><components_bridge><c type="2"><object id="3"><pos x="-16.25" y="11.75"/><in1 component_id="0" node_index="0"/><out1/></object></c><c type="2"><object id="4"><pos x="-16.25" y="11.25"/><in1 component_id="0" node_index="0"/><out1/></object></c><c type="7"><object id="79"><pos x="-7" y="12"/><in1 component_id="76" node_index="1"/><out1/></object></c></components_bridge><groups/></group></microprocessor_definition><vp x="22" y="0" z="-18"/><logic_slots><slot editor_connected="0" value="0"/><slot editor_connected="0" value="0"/><slot editor_connected="0"><value/></slot></logic_slots></o></c><c d="mic" t="0"><o r="1,0,0,0,1,0,0,0,1" bc="FFFFFFFF" ac="FFFFFFFF" sc="5" custom_name="" sensitivity="0.2" timer_scalar_1="0.183" timer_scalar_2="0.183" lss_mode="0" func_type="0" property_text="" is_inverted="false"><vp x="27" y="0